{
  "expo": {
    "name": "StoryStreaks",
    "slug": "StoryStreaks",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ChoreyStoriesLogo.jpeg",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "extra": {
      "defaultAIProvider": "gemini",
      "openaiApiKey": "sk-your-openai-key-here",
      "geminiApiKey": "your-gemini-key-here",
      "claudeApiKey": "your-claude-key-here",
      "eas": {
        "projectId": "391c25a3-631e-4cc0-8410-f3e1a07969e8"
      }
    },
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.storystreaks.app",
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to camera to take photos for chore completion.",
        "NSPhotoLibraryUsageDescription": "This app needs access to photo library to select photos for chore completion.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.storystreaks.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/ChoreyStoriesLogo.jpeg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/ChoreyStoriesLogo.jpeg"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them for chore completion.",
          "cameraPermission": "The app accesses your camera to let you take photos for chore completion."
        }
      ]
    ],
    "owner": "devdzo"
  }
}
